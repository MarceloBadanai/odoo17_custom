<odoo>
    <data>
        <template id="device_doc">
            <t t-call="web.external_layout">
                <div class='mt-5'>
                    <div class="page">
                        <h2>Device's report</h2>
                    </div>
                </div>
            </t>
        </template>        
        <template id="device_report">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as= "o">
                    <t t-call="device_doc" />
                </t>
            </t>
        </template>        
    </data>
</odoo>